category:            Data
build-type:          Simple
extra-source-files:  README.org, README.md
cabal-version:       >=1.10

library
  exposed-modules:     Data.ML.DecisionTree, Data.ML.RandomForest, Data.ML.MisclassificationRate, Data.ML.DecisionTree.Type, Data.ML.DecisionTree.Ops, Data.ML.DecisionTree.Test, Data.ML.DecisionTree.Pretty, Data.ML.Util.Util, Data.ML.CrossValidation.Type, Data.ML.RandomForest.Type, Data.ML.RandomForest.Test, Data.ML.RandomForest.Pretty, Data.ML.RandomForest.Ops, Data.ML.MisclassificationRate.Ops, Data.ML.TestData.BankTrain, Data.ML.TestData.BankType
  -- other-modules:       
  other-extensions:    ScopedTypeVariables
  build-depends:       base >=4.7 && <4.8, containers >=0.5 && <0.6, random >=1.1 && <1.2, ansi-wl-pprint >=0.6 && <0.7, MonadRandom >=0.3 && <0.4, hmatrix >=0.16 && <0.17
  hs-source-dirs:      src
  default-language:    Haskell2010